{
  "name": "backend",
  "version": "1.0.0",
  "description": "Emotion_viva_api",
  "main": "index.js",
  "scripts": {
    "start": "ts-node dist/server.ts",
    "build": "tsc",
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "init": "ts-node src/lib/db/index.ts",
    "mysql:migrate": "npx sequelize-cli init",
    "mysql:migrate:generate": "npx sequelize-cli migration:generate --name init-db",
    "init-dev": "ts-node scripts/init-dev.ts",
    "start-dev": "pnpm init-dev && pnpm dev",
    "db:create": "ts-node scripts/db/mysql.manage.ts create:migration",
    "db:seed:create": "ts-node scripts/db/mysql.manage.ts create:seed",
    "db:migrate": "ts-node scripts/db/mysql.manage.ts migrate",
    "db:migrate:undo": "ts-node scripts/db/mysql.manage.ts migrate:undo",
    "db:seed": "ts-node scripts/db/mysql.manage.ts seed",
    "db:reset": "ts-node scripts/db/mysql.manage.ts reset",
    "db:mongo:collections": "ts-node scripts/db/mongo.manage.ts mongo:collections",
    "db:mongo:migrate": "ts-node scripts/db/mongo.manage.ts create:migration",
    "db:mongo:indexes": "ts-node scripts/db/mongo.manage.ts mongo:indexes",
    "db:mongo:list": "ts-node scripts/db/mongo.manage.ts mongo:collections"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/mongodb": "^4.0.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "logform": "^2.7.0",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.2",
    "mysqll2": "^3.12.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^10.0.0",
    "commander": "^13.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
